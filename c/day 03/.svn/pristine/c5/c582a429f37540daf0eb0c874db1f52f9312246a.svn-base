#include <stdio.h>
#include <stdlib.h>

char *my_strchr(char *str, char c)
{
    int i = 0;
    while (str[i] != '\0') {
        if (c == str[i]) {
            return &(str[i]);
     
        }
        i++;
    }
    return (NULL);
}
